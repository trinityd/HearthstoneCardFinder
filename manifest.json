{
	"manifest_version": 2,
	"name": "Hearthstone Card Finder",
	"short_name": "Hearthstone Card Finder",
	"description": "Search Hearthstone cards",
	"version": "0.1.2",
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"libs/jquery.min.js"
			]
		}
	],
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html",
		"default_title": "Hearthstone Card Finder"
	}
}